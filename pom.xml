                <groupId>com.gitee.pifeng</groupId>
                <artifactId>monitoring-common-web</artifactId>
                <version>${revision}</version>
            </dependency>
            <!-- 监控客户端父项目 -->
            <dependency>
                <groupId>com.gitee.pifeng</groupId>
                <artifactId>monitoring-client</artifactId>
                <version>${revision}</version>
            </dependency>
            <!-- 监控客户端基础模块 -->
            <dependency>
                <groupId>com.gitee.pifeng</groupId>
